<section class="dashboard dashboard-config">
    <div class="row">
        <aside class="col s2">
            {{> sidebar NewMail=Data.NewMail }}
        </aside>

        <div class="col s10 dashboard-grid">
            <div class="row">
                <div class="card-panel">
                    <div class="row valign-wrapper">
                        <div class="col s12">
                            <h4 class="grey-text">
                                <i class="fa fa-cog"></i>
                                Config
                            </h4>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col s12">
                            <form action="">
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="primary-phone-number" type="tel" class="validate" required>
                                        <label for="primary-phone-number">Primary phone number</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="secondary-phone-number" type="tel" class="validate" required>
                                        <label for="secondary-phone-number">Secondary phone number</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="fixed-phone-number" type="tel" class="validate" required>
                                        <label for="fixed-phone-number">Fixed phone number</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="email" type="email" class="validate" required>
                                        <label for="email">Email</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="facebook-page" type="url" class="validate" required>
                                        <label for="facebook-page">Facebook page</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="instagram-page" type="url" class="validate" required>
                                        <label for="instagram-page">Instagram page</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="youtube-channel" type="url" class="validate" required>
                                        <label for="youtube-channel">Youtube channel</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="password" type="password" class="validate" required>
                                        <label for="password">Password</label>
                                    </div>
                                </div>
                                <div class="row form-controls">
                                    <div class="col s6 center-align">
                                        <input type="submit" class="btn btn-large waves-effect waves-light" value="Submit">
                                    </div>
                                    <div class="col s6 center-align">
                                        <input type="reset" class="btn btn-large waves-effect waves-light" value="Reset">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>