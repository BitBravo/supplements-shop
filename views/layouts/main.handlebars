<!DOCTYPE html>

<html lang="ar">

<head>

    {{!-- Meta data. --}}
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="author" content="EOussama">
    <meta name="application-name" content="Supplements Maroc">
    <meta name="description" content="Online store for selling workout-related protein products.">
    <meta name="keywords" content="store, e-commerce">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {{!-- The favicon. --}}
    <link rel="icon" href="/assets/img/favicon.png">

    {{!-- FontAwesome. --}}
    <link rel="stylesheet" href="/assets/fontawesome/css/all.min.css">

    {{!-- Materialize Stylesheet. --}}
    <link rel="stylesheet" href="/assets/materialize/dist/css/materialize.min.css">

    {{!-- Material Icons. --}}
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

    {{!-- The main stylesheet. --}}
    <link rel="stylesheet" href="/assets/css/main.css">

    {{!-- JQuery Script. --}}
    <script src="/assets/jquery/dist/jquery.min.js"></script>

    {{!-- Materialize Script. --}}
    <script src="/assets/materialize/dist/js/materialize.min.js"></script>

    {{!-- The main script. --}}
    <script src="/assets/js/main.js"></script>

    {{!-- The contact script. --}}
    <script src="/assets/js/contact.js"></script>

    {{!-- The title. --}}
    <title>Supplements Maroc</title>
</head>

<body>

    {{!-- The navbar. --}}
    <div class="navbar-fixed">
        <nav class="nav-extended grey darken-4">

            {{!-- Upper navbar. --}}
            <div class="nav-wrapper container">

                <div class="left">
                    <a href="/" class="left brand-logo" title="Supplement Maroc">SM</a>
                </div>

                <div class="right">
                    <a href="javascript:void(0)" data-target="nav-sidebar" class="right sidenav-trigger">
                        <i class="material-icons">menu</i>
                    </a>
                </div>

                <div class="right hide-on-med-and-down">

                    {{!-- Phone number. --}}
                    <a class="grey-text" href="javascript:void(0)">
                        <i class="fa fa-phone nav-phone nav-icon"></i>
                        <b>{{Data.Config.Phone.Primary}}</b>
                    </a>

                    {{!-- Social icons. --}}
                    <a class="left nav-social grey-text" href="{{Data.Config.Facebook.Link}}" target="_blank">
                        <i class="fab fa-facebook-f"></i>
                    </a>
                    <a class="left nav-social grey-text" href="{{Data.Config.Instagram.Link}}" target="_blank">
                        <i class="fab fa-instagram"></i>
                    </a>
                    <a class="left nav-social grey-text" href="{{Data.Config.Youtube.Link}}" target="_blank">
                        <i class="fab fa-youtube"></i>
                    </a>
                </div>
            </div>

            {{!-- Lower navbar. --}}
            <nav class="grey darken-3 hide-on-med-and-down">
                <div class="nav-wrapper container">
                    <ul class="left">
                        <li><a href="/">الصفحة الرئيسية</a></li>
                        <li><a href="/products">جميع المنتوجات</a></li>
                        <li><a href="javascript:void(0)" class="contact-btn">اتصل بنا</a></li>
                        <li><a href="javascript:void(0)" class="about-btn">معلومات عنا</a></li>
                    </ul>
                    <ul class="right">
                        <li class="search-btn">
                            <a href="javascript:void(0)">
                                <i class="fa fa-search nav-icon"></i>
                            </a>
                        </li>
                        <li>
                            <a href="javascript:void(0)" class="cart-button">
                                <i class="fa fa-shopping-cart nav-icon"></i>
                                <small class="red cart-button__badge">0</small>
                            </a>
                        </li>
                        <li><a href="/dashboard"><i class="fa fa-cog nav-icon"></i></a></li>
                    </ul>
                </div>
            </nav>

            {{!-- Small screens' lower navbar. --}}
            <nav class="grey darken-3 hide-on-large-only">
                <div class="nav-wrapper container">
                    <ul class="right">
                        <li>
                            <a href="javascript:void(0)" class="search-button">
                                <i class="fa fa-search nav-icon"></i>
                            </a>
                        </li>
                        <li>
                            <a href="javascript:void(0)" class="cart-button">
                                <i class="fa fa-shopping-cart nav-icon"></i>
                                <small class="red cart-button__badge">0</small>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </nav>
    </div>

    {{!-- Nav sidebar. --}}
    <ul class="sidenav right-align grey lighten-3" id="nav-sidebar">
        <li>
            <a href="/">
                <span dir="ltr">
                    الصفحة الرئيسية
                    <i class="fas fa-home"></i>
                </span>
            </a>
        </li>
        <li>
            <a href="/products">
                <span dir="ltr">
                    جميع المنتوجات
                    <i class="fas fa-prescription-bottle"></i>
                </span>
            </a>
        </li>
        <li>
            <a href="javascript:void(0)" class="contact-btn">
                <span dir="ltr">
                    اتصل بنا
                    <i class="fas fa-at"></i>
                </span>
            </a>
        </li>
        <li>
            <a href="javascript:void(0)" class="about-btn">
                <span dir="ltr">
                    معلومات عنا
                    <i class="fas fa-info-circle"></i>
                </span>
            </a>
        </li>
        <li>
            <a href="/dashboard">
                <span dir="ltr">
                    لوحة القيادة
                    <i class="fas fa-columns"></i>
                </span>
            </a>
        </li>
    </ul>

    {{!-- Contact Modal. --}}
    <div id="contact-modal" class="modal modal-fixed-footer contact-modal">
        <form id="contact-form" class="col s12">
            <div class="modal-content">
                <div class="row">
                    <div class="col s12">
                        <h4 class="right-align">اتصل بنا</h4>
                    </div>
                </div>

                <div class="row contact-modal__content">
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="message-username" type="text" class="right-align validate" data-length="100"
                                name="username" minlength="2" required>
                            <label for="message-username">اسم المستخدم</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="message-email" type="email" class="right-align validate" data-length="80"
                                name="email" required>
                            <label for="message-email">البريد الإلكتروني</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <textarea id="message-message" class="right-align materialize-textarea" data-length="2000"
                                rows="5" name="message" minlength="5" required></textarea>
                            <label for="message-message">رسالة</label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <input type="submit" class="waves-effect waves-green btn-flat" value="إرسال">
                <a href="javascript:void(0)" class="modal-close waves-effect waves-green btn-flat">أغلق</a>
            </div>
        </form>
    </div>

    {{!-- Scroll back to top button. --}}
    <a href="javascript:void(0)" id="scroll-to-top" title="Scroll to top" class="visible scroll-to-top">
        <i class="fas fa-chevron-up fa-2x"></i>
    </a>

    {{!-- The main content of the page. --}}
    <main>
        {{{body}}}
    </main>

    {{!-- The footer's partial. --}}
