                        <li>
                            <a class="grey-text text-lighten-3" href="{{Data.Config.Instagram.Link}}" target="_blank">
                                <i class="fab fa-instagram fa-lg"></i>
                                <span>{{Data.Config.Instagram.Name}}</span>
