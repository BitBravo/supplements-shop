// Importing the dependancies
var app = require('./../app'),
  http = require('http'),
  server = http.createServer(app);

// Listening.
server.listen(process.env.PORT || 3000, () => {
  // Logging.
  console.log(
    `Supplements Maroc has successfully started on port ${process.env.PORT ||
    3000}.`
  );
});
