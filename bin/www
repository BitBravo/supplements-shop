/**
 * Importing the dependancies
 */
var app = require('./../app'),
  http = require('http'),
  server = http.createServer(app);



/**
 * Starting to the app
 */
server.listen(app.get('port'), function () {
  console.log(
    'Supplements Maroc has successfully started on port ' + app.get('port') + '.'
  );
});
